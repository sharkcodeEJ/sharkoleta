// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model item {
  id     Int     @id
  title  String
  image  String
  points point[] @relation(references: [id])
}

model point {
  id          String  @id @default(uuid())
  name        String
  email       String
  description String
  whatsapp    String
  fone        String
  latitude    Decimal
  longitude   Decimal
  city        String
  uf          String
  address     String
  district    String
  number      Int
  cep         String
  image       String
  itens       item[]  @relation(references: [id])
}
